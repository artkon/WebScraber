{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "require-jsdoc": "off",
        "max-len": "off",
        "no-unused-vars": 0,
        "indent": ["error", 4, { "SwitchCase": 1 }]
    },
    "extends": "google"
}