{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "require-jsdoc": "off",
        "max-len": "off",
        "no-unused-vars": 0,
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"no-tabs": 0,
        "object-curly-spacing": [1, "always"]
    },
    "extends": "google"
}
